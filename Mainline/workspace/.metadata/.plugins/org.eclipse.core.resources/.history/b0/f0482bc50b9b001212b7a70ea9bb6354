	float[] zLocOut;
	float[] zLocIntan;
	float[] zLocOuttan;
	
	//ROTATION DATA FOR XYZ
	float[] xRotIn;
	float[] xRotOut;
	float[] xRotIntan;
	float[] xRotOuttan;
	float[] yRotIn;
	float[] yRotOut;
	float[] yRotIntan;
	float[] yRotOuttan;
	float[] zRotIn;
	float[] zRotOut;
	float[] zRotIntan;
	float[] zRotOuttan;
}
